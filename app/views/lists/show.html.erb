<h2><%= @list.name %></h2>
<%# here im guessing youll have to add the bookmarks %>
  <ul>
    <% @list.movies.each do |movie| %>
      <% bookmark = Bookmark.find_by(list: @list, movie: movie) %>
      <li><%= movie.title %><%= link_to "delete", bookmark_path(bookmark), data:{turbo_method: :delete} %></li>
    <% end %>
  </ul>
  <%= link_to "edit", edit_list_path(@list), class: "btn btn-primary" %>
  <%= link_to "new", new_list_bookmark_path(@list), class: "btn btn-primary" %>

